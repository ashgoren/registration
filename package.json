    "eslint-config-react-app": "^7.0.1",
    "fake-indexeddb": "^6.0.1",
    "happy-dom": "^17.4.7",
    "jsdom": "^26.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^3.1.3",
    "yargs": "^17.7.2"
  },
  "type": "module"
}
