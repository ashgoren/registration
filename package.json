  "scripts": {
    "start": "doppler run -- vite",
    "dev": "doppler run -- vite",
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage --watch=false",
    "orders": "doppler run -- node ./scripts/orders.js",
    "missing": "doppler run -- node ./scripts/missing.js",
    "spreadsheet": "doppler run -- node ./scripts/spreadsheet.js",
    "cleanup-artifacts": "doppler run -- node ./scripts/cleanupArtifacts.js",
    "matchPayments": "doppler run -- node ./scripts/matchPayments.js",
    "emulator": "node ./scripts/start-emulator.js",
    "localtunnel-paypal": "lt -p 5001 -s <PROJECT_ID>",
    "localtunnel-stripe": "stripe listen --events payment_intent.succeeded --forward-to localhost:5001/<PROJECT_ID>/us-west1/stripeWebhook",
    "bootstrap": "node scripts/bootstrap/main.js",
    "terraform-stg": "node scripts/bootstrap/terraformWrapper.js stg",
    "terraform-prd": "node scripts/bootstrap/terraformWrapper.js prd",
    "set-payment-secrets": "node scripts/bootstrap/setPaymentSecrets.js",
    "disable-apis": "bash scripts/disable-apis.sh",
    "enable-apis": "bash scripts/enable-apis.sh"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@google-cloud/artifact-registry": "^4.1.0",
    "@vitest/coverage-v8": "^3.1.3",
    "dotenv": "^16.5.0",
